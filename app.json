{
  "expo": {
    "name": "DoneWithIt",
    "slug": "DoneWithIt",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9a888a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codebyron.donewithit",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.codewithmosh.donewithit",
      "version": 1
    },
    "description": "a marketplace for selling the stuff you don't need anymore.",
    "notification": {
      "iosDisplayInForeground": true
    },
    "extra": {
      "bugsnag": {
        "apiKey": "34698e2bfaa7de9dde7b06fa1fec5c59"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    }
  }
}